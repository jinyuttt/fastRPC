public interface INetServer {
    
    /**
     * 
     * @param ip
     */
public void setLocalIP(String ip);
public void setLocalPort(int port);
public boolean start();
public boolean start(IRecvieHander hander);
public byte[] recviceData();
public JYSocket recvice();

}
